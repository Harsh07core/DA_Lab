pipeine {
agent any
stages{
   stage('checkout'){
 steps{
  git branch: 'main',url:''
     }
  }
  stages('Deploy'){
      steps{
        sh'''
          mkdir -p deployed_html
          cp *.html deployed_html/
          ''' 
         }
       }
     }
  }
   post{
     success{
          publishHTML_(target:)
    }
}
